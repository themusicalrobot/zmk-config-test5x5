#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
        keymap {
        compatible = "zmk,keymap";

        default_layer { // Layer 0
            display-name = "Num";
// ----------------------------------------------
// |      layer1      |        Layer2       |      Layer3      |      layer4      |        Layer5       |
// |      Enter       |        7            |      8           |      9           |        /            |
// |      Shift       |        4            |      5           |      6           |        *            |
// |      none        |        none         |      none        |      none        |        none         |
// |      none        |        none         |      none        |      none        |        none         |
            bindings = <
                &tog 0  &tog 1  &tog 2  &tog 3  &tog 4
                &kp RET   &kp 7   &kp 8   &kp 9   &kp SLASH
                &kp LSHIFT   &kp 4   &kp 5   &kp 6   &kp ASTERISK
                &kp ESC   &kp 1   &kp 2   &kp 3   &kp PLUS
                &kp PERCENT   &kp EQUAL   &kp 0   &kp UNDERSCORE   &kp MINUS
            >;
        };
        layer2 { // Layer 2
            display-name = "Code";
// ----------------------------------------------
// |      layer1      |        Layer2       |      Layer3      |      layer4      |        Layer5       |
// |      none        |        none         |      none        |      none        |        none         |
